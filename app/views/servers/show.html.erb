<div class="container">
  <div class="starter-template">
    <h1><%= @server.name %></h1>

    <div id="container" style="width:100%; height:400px;"></div>
  </div>
</div>
<script src="http://code.highcharts.com/highcharts.js"></script>
<script type="text/javascript">
  $(function () {
    $('#container').highcharts({
        chart: {
            type: 'line'
        },
        title: {
            text: 'Last 5 heartbeats CPU usage'
        },
        xAxis: {
            title: 'Time',
            tickInterval: 1
        },
        yAxis: {
            title: {
                text: 'CPU%'
            }
        },
        series: [{
            name: 'Server',
            data: <%= @cpu_data %>
        }]
    });
});
</script>
